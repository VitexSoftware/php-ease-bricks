{
    "name": "vitexsoftware/ease-bricks",
    "description": "EasePHP framework addons and code snipplets",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ease\\ui\\": "src/Ease/ui"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\ui\\": "tests/src/Ease/ui",
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\Ease\\TWB\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease/TWB"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "vitexsoftware/ease-twbootstrap": "*"
    },
    "require-dev": {
        "apigen/apigen": "^4.",
        "robmorgan/phinx": "* dev-master",
        "phpunit/php-token-stream": "~1.2",
        "phpunit/phpunit": "^8",
        "phpunit/phpunit-skeleton-generator": "*",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "0.6.*",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2"
    },
    "type": "library",
    "homepage": "http://v.s.cz/ease.php",
    "license": "GPL-2.0+"
}
