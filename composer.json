        }
    },
    "minimum-stability": "stable",
    "require": {
        "vitexsoftware/ease-html": ">=1.31"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "type": "library",
    "homepage": "http://v.s.cz/ease.php",
    "license": "GPL-2.0+"
}
