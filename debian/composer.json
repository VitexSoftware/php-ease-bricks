{
    "name": "deb/ease-bricks",
    "description": "Addons for PHP Framework (debianized)",
    "version": "0.9",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
